FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
   IN
    {
        type            zeroGradient;
    }
    OUT
    {
        type            fixedMeanValue;
        value           uniform 0;
	meanValue       0;
    }
    SYM1
    {
        type            symmetry;
    }
    SYM2
    {
        type            symmetry;
    }
    CYLINDERWALL
    {
        type            zeroGradient;
    }
    periodic1
    {
        type            cyclic;
    }
    periodic2
    {
        type            cyclic;
    }
    "proc.*"
    {
        type            processor;
    }
}


