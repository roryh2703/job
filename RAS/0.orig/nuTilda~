FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-6;

boundaryField
{
    IN
    {
        type            fixedValue;
        value           uniform 1e-6;
    }
    OUT
    {
        type            inletOutlet;
        inletValue      uniform 1e-6;
        value           uniform 1e-6;
    }
    SYM1
    {
        type           symmetry;
    }
    SYM2
    {
        type           symmetry;
    }
    CYLINDERWALL
    {
        type            fixedValue;
        value           uniform 0;
    }
    periodic1
    {
        type            cyclic;
    }
    periodic2
    {
        type            cyclic;
    }
}


